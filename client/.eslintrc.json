{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["../client"],
    "rules": {
        "object-curly-spacing": ["error", "always"],
        "react/prop-types": 0,
        "require-jsdoc": 0,
        "no-unused-vars": "warn",
        "max-len": ["warn", {"code": 110}]
    }
}